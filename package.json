{
    "name": "simplefileheader",
    "displayName": "simpleFileheader",
    "description": "file comments",
    "version": "0.0.1",
    "publisher": "vamcc",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.simpleFileheader"
    ],
    "main": "./extension",
    "license": "MIT",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "File header Configuration",
            "properties": {
                "fileheader.Author": {
                    "type": "string",
                    "default": "vamcc",
                    "description": "By default, create file username"
                },
                "fileheader.Description": {
                    "type": "string",
                    "default": "file description",
                    "description": "By default, file description"
                }
            }
        },
        "commands": [{
            "command": "extension.simpleFileheader",
            "title": "fileheader"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "repository": {
      "type": "git",
      "url": "git://github.com/vamcc/simpleFileheader.git"
    }
}